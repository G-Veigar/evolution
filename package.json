{
  "name": "evolution2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "analyse": "hiper -c hiper.config.js",
    "build-storybook": "build-storybook",
    "dev": "vue-cli-service serve",
    "img2webp": "node ./scripts/img2webp.js",
    "modernizr": "modernizr -c modernizr.config.json -d src/utils",
    "report": "vue-cli-service build --report",
    "storybook": "start-storybook -p 6006",
    "webpack": "vue inspect > webpack.dev.js --mode development && vue inspect > webpack.prod.js --mode production",
    "webpack:plugin": "vue inspect > webpack.dev.plugin.js --plugins --mode development && vue inspect > webpack.prod.plugin.js --plugins --mode production"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.1.2",
    "mand-mobile": "^2.5.3",
    "modern-normalize": "^0.5.0",
    "vconsole": "^3.3.2",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/vue": "^5.2.4",
    "@vue/cli-plugin-babel": "^4.0.4",
    "@vue/cli-plugin-eslint": "^4.0.4",
    "@vue/cli-plugin-unit-jest": "^4.0.4",
    "@vue/cli-service": "^4.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "babel-preset-vue": "^2.0.2",
    "chokidar": "^3.2.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "image-webpack-loader": "^5.1.0",
    "lint-staged": "^9.4.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prerender-spa-plugin": "^3.4.0",
    "sass": "^1.23.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "scripts/**/*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "src/**/*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
